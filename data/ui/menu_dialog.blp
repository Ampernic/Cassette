using Gtk 4.0;
using Adw 1;

template $CassetteMenuDialog : Adw.Dialog {
  presentation-mode: bottom_sheet;

  Adw.ToolbarView {
    top-bar-style: flat;

    [top]
    Adw.HeaderBar {
      title-widget: Adw.WindowTitle window_title {
        title: "Test";
      };
    }

    content: Adw.Clamp clamp {
      margin-bottom: 12;
      margin-end: 12;
      margin-start: 12;
      margin-top: 12;
    };
  }
}